# qa-executor Environment Variables

# qa-tools paths
QA_TOOLS_CORE_DIR=~/Src/qa-tools/qa-tools-core
QA_TOOLS_DESIGNER_DIR=~/Src/qa-tools/qa-tools-designer
QA_USE_CASES_DIR=~/Src/qa-tools/qa-use-cases
QA_TOOLS_BOARD_DIR=~/Src/qa-tools/qa-tools-board

# Discord webhook (for agent deployment)
DISCORD_WEBHOOK_QA_CORE=https://discord.com/api/webhooks/...
DISCORD_WEBHOOK_QA_WORK=https://discord.com/api/webhooks/...

# OpenClaw (if running as agent)
OPENCLAW_WORKSPACE=~/Src/qa-tools/qa-executor
OPENCLAW_AGENT_NAME=qa-executor
OPENCLAW_AGENT_ROLE=worker

# Default models
QA_TOOLS_DEFAULT_MODEL=openai:gpt-4o
QA_TOOLS_EVAL_MODEL=openai:gpt-4o

# Lake paths
QA_TOOLS_LAKE_DIR=~/Src/qa-tools/qa-tools-core/lake
QA_TOOLS_ARTIFACTS_DIR=~/Src/qa-tools/qa-tools-core/lake/artifacts
